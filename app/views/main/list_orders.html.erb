<a href="/">Back to Order</a>

<h1>orders</h1>

<table border="0">

  <tr>
    <th>ID</th>
    <th>Price</th>
    <th>Ordered at</th>
  </tr>

  <% @flyer_quote.each do |order| %>
    <tr>
      <td><%= order.id %></td>
      <td>$<%= "%.2f" % order.total_price %></td>
      <td><%= order.created_at %></td>
    </tr>
  <% end %>

</table>
